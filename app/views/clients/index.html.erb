<p id="notice"><%= notice %></p>

<h1>Clients</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cpf</th>
      <th>Email</th>
      <th>Cep</th>
      <th>Public place</th>
      <th>District</th>
      <th>Number house</th>
      <th>Complement</th>
      <th>City</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.name %></td>
        <td><%= client.cpf %></td>
        <td><%= client.email %></td>
        <td><%= client.cep %></td>
        <td><%= client.public_place %></td>
        <td><%= client.district %></td>
        <td><%= client.number_house %></td>
        <td><%= client.complement %></td>
        <td><%= client.city %></td>
        <td><%= client.state %></td>
        <td><%= link_to 'Show', client %></td>
        <td><%= link_to 'Edit', edit_client_path(client) %></td>
        <td><%= link_to 'Destroy', client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client', new_client_path %>
